<Window x:Class="HaushaltsäquivalenteWPFApp.DetailPersonWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HaushaltsäquivalenteWPFApp"
        mc:Ignorable="d"
        Title="DetailPersonWindow" Height="450" Width="250" Loaded="Window_Loaded">
    <Grid>
        <StackPanel Margin="5">
            <TextBlock x:Name="NameHeadline" FontSize="18" FontWeight="Bold"></TextBlock>
            <TextBlock x:Name="LastWeek"></TextBlock>
            <TextBlock x:Name="LastMonth"></TextBlock>
            <TextBlock x:Name="LastYear"></TextBlock>
        </StackPanel>
        
    </Grid>
</Window>
